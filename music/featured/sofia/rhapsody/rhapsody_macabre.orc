;=================================================================================;;Rhapsody Macabre.orc 															  ;       ;=================================================================================;;"Rhapsody Macabre"   by Sal Sofia (©) 2000					                      ;              					              ;;=================================================================================;sr = 44100 kr = 4410                                 ksmps = 10nchnls = 2;===================================< initialization >============================;gallp     init 0 ;;instr 94gallp2    init 0 ;;instr 95garvbsig3  init 0 ;;instr 96garvbsig4  init 0 ;;instr 97garvbsig1  init 0 ;;instr 98garvbsig2  init 0 ;;instr 99ga1 init 0  ;reverb instr 199ga2 init 0  ;reverb instr 199;=================================================================================;;==========================================< instr 1 >============================;      instr 1idb		=	ampdb (p4)isc		=	(idb * 0.333)k1 line 100, p3, 1000 k2 line 1000, p3, 100k3 linen isc, p6, p3, p7k4 line 1000, p3, 50 k5 line 50, p3, 1000  k6 linen isc, p6, p3, p7a5 oscili k3, p8, p5a6 oscili k3, (p8 * 0.999), p5a7 oscili k3, (p8 * 1.001), p5   a1 = a5 + a6 + a7  a8 oscili k6, p9, p5a9 oscili k6, (p9 * 0.999), p5a10 oscili k6, (p9 * 1.001), p5   a11 = a8 + a9 + a10a2 butterbp a1, k1, 50a3 butterbr a2, k4, 25   a4 balance a3, a1a12 butterbp a11, k2, 50a13 butterbr a12, k5, 25   a14 balance a13, a11	outs a4, a14ga1   =	ga1 + a4 ga2   =	ga2 + a14 	endin;===========================================< instr 2 >===========================;      instr 2idb1	=	ampdb (p9)  idb2	=	ampdb (p11) irvgain1	=	p12irvgain2	=	p13k1 oscili idb1, 1 / p3, p8k2 oscili idb2, 1 / p3, p10k3 linen  3, 1, p3, 0.3k4 randi k3, 9, 10k5 randi k3, 13, 15k6 linen p20, p14, p3, p16k7 linen p21, p15, p3, p17k8 oscili k6, p18, 1k9 oscili k7, p19, 1a1 oscili k1, p4 + k4 + k8, p5a2 oscili k2, p6 + k5 + k9, p7	outs a1,a2garvbsig1 = garvbsig1 + (a1 * irvgain1)garvbsig2 = garvbsig2 + (a2 * irvgain2)	endin;===========================================< instr 5 >===========================;      instr 5idb = ampdb (p4)isc = (idb * 0.333)      k1 line 100, p3, 1000        k2 line 1000, p3, 100       k3 linen isc, p6, p3, p7     k4 line 1000, p3, 50       k5 line 50, p3, 1000         k6 linen isc, p6, p3, p7      a5 oscili k3, p8, p5a6 oscili k3, (p8 * 0.999), p5a7 oscili k3, (p8 * 1.001), p5	a1 = a5 + a6 + a7      a8 oscili k6, p9, p5a9 oscili k6, (p9 * 0.999), p5a10 oscili k6, (p9 * 1.001), p5	a11 = a8 + a9 + a10      a2 butterbp a1, k1, 50 a3 butterbr a2, k4, 25 	a4 balance a3, a1      a12 butterbp a11, k2, 50 a13 butterbr a12, k5, 25 	a14 balance a13, a11      	outs a4, a14gallp  = (a4 * p10)gallp2 = (a14 * p10)     garvbsig3 = garvbsig3 + (a4 * p10)garvbsig4 = garvbsig4 + (a14 * p10)      endin   ;===========================================< instr 6 >===========================;instr 6idb = ampdb (p4)isc = (idb * 0.333)      k1 line 10, p3, 100        k2 line 100, p3, 10        k3 linen isc, p6, p3, p7k4 line 100, p3, 5       k5 line 5, p3, 100       k6 linen isc, p6, p3, p7      a5 oscili k3, p8, p5a6 oscili k3, (p8 * 0.999), p5a7 oscili k3, (p8 * 1.001), p5   a1 = a5 + a6 + a7      a8 oscili k6, p9, p5a9 oscili k6, (p9 * 0.999), p5a10 oscili k6, (p9 * 1.001), p5    a11 = a8 + a9 + a10      a2 butterbp a1, k1, 500 a3 butterbr a2, k4, 250    a4 balance a3, a1      a12 butterbp a11, k2, 500 a13 butterbr a12, k5, 250     a14 balance a13, a11            outs a4, a14   gallp  = a4 * p10   gallp2 = a14 * p10      endin   ;======================================<instr 94 >=============================;     instr 94ihz1    =        1 / 6ihz2    =        1 / 9.7ihz3    =        1 / 13.1ihz4    =        1 / 14.73ihz5    =        1 / 249ihz6    =        1 / 418asig    =        gallp   alp1    alpass   asig, 6, ihz1alp2    alpass   alp1, (6 * 0.909), ihz2alp3    alpass   alp2, (6 * 0.877), ihz3alp4    alpass   alp3, (6 * 0.768), ihz4alp5    alpass   alp4, (6 * 0.643), ihz5alp6    alpass   alp5, (6 * 0.531), ihz6aenv    linen    alp6, 0.003, p3, 0.4   outs     aenv, (aenv * 0)   gallp = 0    endin  ;======================================<instr 95 >=============================;  instr 95    ihz1    =        1 / 12ihz2    =        1 / 49.7ihz3    =        1 / 143.1ihz4    =        1 / 274.73ihz5    =        1 / 1149ihz6    =        1 / 2418asig    =        gallp2  alp1    alpass   asig, 1.6, ihz1alp2    alpass   alp1, (1.6 * 0.909), ihz2alp3    alpass   alp2, (1.6 * 0.877), ihz3alp4    alpass   alp3, (1.6 * 0.768), ihz4alp5    alpass   alp4, (1.6 * 0.643), ihz5alp6    alpass   alp5, (1.6 * 0.531), ihz6aenv    linen    alp6, 0.003, p3, 0.2    outs     aenv * 0, aenv     gallp2 = 0    endin       ;===============================<instr 96 - 97 reverb>============================;instr 96irvtime = p4a1  reverb2 garvbsig3, irvtime, 0.9k1  line  50, p3, 2500a13 butterbr a1, k1, 10	a14 balance a13, a1 	outs a14, (a14) * 0garvbsig3 = 0	endininstr 97irvtime = p4a1  reverb2 garvbsig4, irvtime, 0.1 k1  line 2500, p3, 100a13 butterbr a1, k1, 50	a14 balance a13, a1 	outs (a14) * 0, a14garvbsig4 = 0	endin;===============================<instr 98 - 99 reverb>============================;instr 98irvtime = p4a1  reverb2 garvbsig1, irvtime, 0.5	outs a1, (a1) * 0garvbsig1 = 0	endininstr 99irvtime = p4a1  reverb2 garvbsig2, irvtime, 0.25	outs (a1) * 0, a1garvbsig2 = 0	endin    ;================================<instr 199 reverb>===============================;instr 199                 irevfactor    =   p4ilowpass      =   9000ioutputscale  =   p5idel1  = 1237.000 / sridel2  = 1381.000 / sridel3  = 1607.000 / sridel4  = 1777.000 / sridel5  = 1949.000 / sridel6  = 2063.000 / sridel7  = 307.000 / sridel8  = 97.000 / sridel9  = 71.000 / sridel10 = 53.000 / sridel11 = 47.000 / sridel12 = 37.000 / sridel13 = 31.000 / sricsc1 = .822 * irevfactoricsc2 = .802 * irevfactoricsc3 = .773 * irevfactoricsc4 = .753 * irevfactoricsc5 = .753 * irevfactoricsc6 = .753 * irevfactoricsc7 = .7 * irevfactoracomb1  comb    ga1, icsc1, idel1acomb2  comb    ga1, icsc2, idel2acomb3  comb    ga1, icsc3, idel3acomb4  comb    ga1, icsc4, idel4acomb5  comb    ga1, icsc5, idel5acomb6  comb    ga1, icsc6, idel6acomball = acomb1 + acomb2 + acomb3 + acomb4 + acomb5 + acomb6allp1   alpass  acomball, icsc7, idel7allp2   alpass  allp1, icsc7, idel8allp3   alpass  allp2, icsc7, idel9alow    tone    allp3, ilowpassallp4   alpass  alow, icsc7, idel10allp5   alpass  allp4, icsc7, idel12arevout1  =  allp5 * ioutputscaleacomb1  comb    ga2, icsc1, idel1acomb2  comb    ga2, icsc2, idel2acomb3  comb    ga2, icsc3, idel3acomb4  comb    ga2, icsc4, idel4acomb5  comb    ga2, icsc5, idel5acomb6  comb    ga2, icsc6, idel6acomball = acomb1 + acomb2 + acomb3 + acomb4 + acomb5 + acomb6allp1   alpass  acomball, icsc7, idel7allp2   alpass  allp1, icsc7, idel8allp3   alpass  allp2, icsc7, idel9alow    tone    allp3, ilowpassallp4   alpass  alow, icsc7, idel10allp6   alpass  allp4,icsc7, idel13arevout2   =  allp6 * ioutputscaleouts arevout1, arevout2ga1 = 0ga2 = 0endin;============================< end >===========================================;