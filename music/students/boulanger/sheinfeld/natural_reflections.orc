; Natural Reflections; by; Freddy Sheinfeldsr        =         44100kr        =         4410ksmps     =         10nchnls    =          2;===================; Global Reverb;===================garvbsig  init      0          instr 500a1        reverb2   garvbsig, p4, p5          outs      a1,a1garvbsig  =         0          endin           										     ;=====================================; Sine w/chorus and gate;=====================================                    		instr	99idur	=		p3iamp	=		ampdb(p4)ifrq	=		cpspch(p5)ifun	=		p6iatk	=		p7irel	=		p8iatkfun	=		p9imodp1	=		p10imodp2	=		p11imodfr1	=		p12imodfr2	=		p13imodfun	=		p14	kpan	=		p15	krev	=		p16		kenv	envlpx	iamp, iatk, idur, irel, iatkfun, .7, .01kmodpth	expon	imodp1, idur, imodp2kmodfrq	line	cpspch(imodfr1), idur, cpspch(imodfr2) 		alfo	oscil	kmodpth, kmodfrq, 1	aosc  	oscil	alfo, ifrq, ifunasig	=		aosc*kenv garvbsig = garvbsig + asig*krev       	outs  	asig*kpan,asig*(1-kpan)		endin													 ;=====================================; Bubles ;=====================================                 instr 100kfreq     =          p5ipanfrq   =  		 p6irandfrq  =      	 p7irandamp  =     	 p8   iyesno	  = 		 p10                                     kramp     linseg    0,p3*.8,p4,p3*.2,0        kenv1     linen     p4,0, p3,10             krndmod   randh     irandamp,irandfrqanoise    randi     kramp,kenv1ao      oscil     anoise,kfreq,11          kpan      oscil     1,.09*ipanfrq,1aosc1     reson     ao,kpan+100+krndmod,100,2       kenv	  oscil 1, 1/p3, 19aosc2      = aosc1*kenv          outs      aosc2*kpan,aosc2*(1-kpan)          endin                       ;=====================================;  Wind;=====================================                 instr 1001kfreq     =          p5ipanfrq   =  		 p6irandfrq  =      	 p7irandamp  =     	 p8   iyesno	  = 		 p10                                     kramp     linseg    0,p3*.8,p4,p3*.2,0     kenv1     linen     p4,0, p3,10              krndmod   randh     irandamp,irandfrqanoise    randi     kramp,kenv1ao        oscil     anoise,kfreq,11         kpan      oscil     1,.09*ipanfrq,1aosc1     reson     ao,kpan+100+krndmod,100,2     kenv	  oscil 1, 1/p3, 6aosc2     = aosc1*kenv          outs      aosc2,aosc2garvbsig  =        garvbsig+(aosc2*.2)*iyesno           endin                             ;=====================================; FM   Sampling  "Natural Pad";===================================== 				instr 	106		iamp	=		ampdb(p4)ifrq	=		cpspch(p5)ifrq2	=		cpspch(p8)ifrq3	=		cpspch(p9)kpan	=       p10krev    =	    p12k1      oscil   iamp, 1/p3, p7k3		line	p10, p3, p11a1 		foscil  k1, ifrq, 1, .5, k3, 1kenv	oscil   1,1/p3,19aout    =   kenv*a1			outs 	aout*kpan,aout*(1-kpan)garvbsig = garvbsig + aout*krev		endin    				;=====================================;   Grain ;===================================== 					instr 	107k2      linseg  p5, p3/2, p9, p3/2, p5k3      line    p10, p3, p11k4      line    p12, p3, p13k5      expon   p14, p3, p15k6      expon	p16, p3, p17a1 		grain 	p4, k2, k3, k4, k5, k6, 1, p6, 1a2      linen   a1, p7, p3, p8		outs 	a2,a2		endin		 	;=================================================;Noise to Melody;=================================================			           instr 108idur	  = 	p3iamp	  =		ampdb(p4)icutf	  =		p5    ibandinit =		p6   ibandmed  =		p7	  ibandend  =		p8	  ifun	  =		p9icutf2	  =		p10kband	  linseg ibandinit , idur*(.3), ibandmed, idur*(.20), ibandend, idur*(.2),ibandmed,idur*(.05),ibandend,idur*(.05),ibandmed, idur*(.2),ibandinitkfrqenv	  linseg icutf, idur*(3/4),icutf2kenv	  oscil  iamp,1/idur,ifunanoise	  rand kenv,5000 aresnoise reson anoise, kfrqenv, kband  ;kbandaout	  = aresnoise irev	  = 0garvbsig  = garvbsig + aout*irevkenvfrq	  linseg 0.03, p3/2, 0.04, p3/2, .02kpan	  oscil   .5, kenvfrq, 1kpanlfo	  = kpan + .5outs      aout*kpanlfo, aout*(1-kpanlfo)          endin		            ;=================================================;Lead Synth;=================================================  		 instr   110idur	 =		p3iamp	 =		ampdb(p4)ifrq 	 = 		cpspch(p5)ifun	 = 		p6iatk	 =		p7irel	 =		p8iatkfun	 =		p9				kcut	 =		p11kpan	 =		p12krev	 =		p13kenv	 envlpx	iamp, iatk, idur, irel, iatkfun, .7, .01asyn1 	 pluck 	kenv, ifrq *.99, ifrq,     ifun, 3, 0.01 asyn2    pluck	kenv, ifrq *.97, ifrq*.98, ifun, 3, 0.01asig1	 butterbp asyn1,kcut,kcut*.2asig2	 butterbp asyn2,kcut,kcut*.2amix	 = asig1 + asig2garvbsig = garvbsig + amix*krev		 outs     kpan*amix, amix*(1-kpan)							 		 endin		      		           		 ;=================================================;NOISE TO MELODY  II;=================================================			           instr 1002idur	  = 		p3iamp      =		ampdb(p4)icutf	  =		p5    ibandinit =		p6    ibandmed  =		p7	  ibandend  =		p8	  ifun	  =		p9icutf2	  =		p10kband	  linseg ibandinit , idur/2, ibandmed, idur/2, ibandmedkfrqenv	  linseg icutf, idur*(3/4),icutf2kenv	  oscil  iamp,1/idur,ifunanoise	  rand kenv,5000 aresnoise reson anoise, kfrqenv, kband  ;kbandaout	  = aresnoise irev	  = 0garvbsig  = garvbsig + aout*irevkenvfrq	  linseg 0.03, p3/2, 0.04, p3/2, .02kpan	  oscil   .5, kenvfrq, 1kpanlfo	  = kpan + .5outs      aout, aout*(1-kpanlfo)           endin		 