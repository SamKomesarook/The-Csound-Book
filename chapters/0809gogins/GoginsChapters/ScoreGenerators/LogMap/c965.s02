a31 = a11 * a1  * sqrt(a21)
a32 = a12 * a2  * sqrt(a21)
a33 = a13 * a3  * sqrt(a21)
a34 = a14 * a4  * sqrt(a21)
a35 = a15 * a5  * sqrt(a21)
