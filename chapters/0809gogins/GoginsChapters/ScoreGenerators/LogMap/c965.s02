; Power to partials.
k2 	linseg 	1.69, .1, 1.75, p3 -.1, 1.69
; AUDIO
a1 	foscili	k1, ifrequency, 1, 2.01, k2, 1
	outs 	a1 * ileftpan, a1 * irightpan


